{
  "openapi": "3.0.1",
  "info": {
    "title": "RelayRunner",
    "version": "1.0"
  },
  "paths": {
    "{urlPrefix}/api/loadClients": {
      "get": {
        "tags": [ "LoadClients" ],
        "summary": "Search LoadClients",
        "description": "Returns an array of `LoadClient` documents optionally filtered by query string parameters",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Filter by search on LoadClient.Name<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "Load-Client-ID-001"
            }
          },
          {
            "name": "id",
            "in": "query",
            "description": "Filter by id on LoadClient.Id<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 1
            }
          },
          {
            "name": "version",
            "in": "query",
            "description": "Filter by version on LoadClient.Version<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 5.0
            }
          },
          {
            "name": "region",
            "in": "query",
            "description": "Filter by region on LoadClient.Region<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "california"
            }
          },
          {
            "name": "zone",
            "in": "query",
            "description": "Filter by zone on LoadClient.Zone<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "colorado"
            }
          },
          {
            "name": "sceduler",
            "in": "query",
            "description": "Filter by search on LoadClient.Scheduler<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "nev"
            }
          },
          {
            "name": "status",
            "in": "query",
            "description": "Filter by status on LoadClient.CurrStatus<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "ready"
            }
          },
          {
            "name": "metrics",
            "in": "query",
            "description": "Filter by metrics on LoadClient.Metrics<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "prometheus"
            }
          },
          {
            "name": "date created",
            "in": "query",
            "description": "Filter by date created on LoadClient.DateCreated<br><br>`valid: greater than 2 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 2019
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Array of `Load Client` documents or empty array if not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/LoadClient"
                  }
                },
                "example": [
                  {
                    "name": "Load-Client-ID-001",
                    "id": 1,
                    "version": 5.0,
                    "region": "california",
                    "zone": "colorado",
                    "scheduler": "nev",
                    "currstatus": "ready",
                    "metrics": "prometheus",
                    "dateCreated": 2019
                  },
                  {
                    "name": "Load-Client-ID-002",
                    "id": 2,
                    "version": 5.0,
                    "region": "california",
                    "zone": "colorado",
                    "scheduler": "nev",
                    "currstatus": "unresponsive",
                    "metrics": "prometheus",
                    "dateCreated": 2021
                  }
                ]
              }
            }
          },
          "400": {
            "description": "Invalid parameter",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Invalid {queryString} parameter"
                }
              }
            }
          }
        }
      }
    },
    "{urlPrefix}/healthz": {
      "get": {
        "tags": [ "Health Check" ],
        "summary": "Health Check (simple)",
        "description": "Returns a text/plain health status (pass, warn or fail)",
        "responses": {
          "200": {
            "description": "`string` (pass, warn or fail)",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                },
                "example": "pass"
              }
            }
          }
        }
      }
    },
    "{urlPrefix}/healthz/ietf": {
      "get": {
        "tags": [ "Health Check" ],
        "summary": "Health Check (IETF)",
        "description": "Returns an `IetfHealthCheck` document from the Health Check",
        "responses": {
          "200": {
            "description": "`IetfHealthCheck`",
            "content": {
              "application/health+json": {
                "schema": {
                  "$ref": "#/components/schemas/IetfHealthCheck"
                },
                "example": {
                  "status": "pass",
                  "serviceId": "relayrunner",
                  "description": "RelayRunner Health Check",
                  "instance": "0",
                  "version": "1.0.0502.0026",
                  "checks": {
                    "getLoadClientById:responseTime": {
                      "status": "pass",
                      "componentId": "getLoadClientById",
                      "componentType": "datastore",
                      "observedUnit": "ms",
                      "observedValue": 1.4,
                      "targetValue": 400,
                      "time": "2020-03-02T01:49:17Z"
                    },
                    "searchLoadClient:responseTime": {
                      "status": "pass",
                      "componentId": "searchLoadClient",
                      "componentType": "datastore",
                      "observedUnit": "ms",
                      "observedValue": 38.11,
                      "targetValue": 400,
                      "time": "2020-03-02T01:49:17Z"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
